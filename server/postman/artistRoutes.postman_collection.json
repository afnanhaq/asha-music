				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"pm.test(\"Song and cover URLs uploaded\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.value.songURL).to.eql(globals['songURL'].replace('\"', '').replace('\"', ''));",
							"    pm.expect(jsonData.value.coverURL).to.eql(globals['coverURL'].replace('\"', '').replace('\"', ''));",
