{
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.4",
    "jsdoc": "^3.6.7"
  },
  "scripts": {
    "cl": "eslint client/src",
    "sl": "eslint server",
    "routes": "newman run server/postman/userRoutes.postman_collection.json --globals server/postman/globals.postman_globals.json ; newman run server/postman/artistRoutes.postman_collection.json --globals server/postman/globals.postman_globals.json",
    "uroutes": "newman run server/postman/userRoutes.postman_collection.json --globals server/postman/globals.postman_globals.json",
    "aroutes": "newman run server/postman/artistRoutes.postman_collection.json --globals server/postman/globals.postman_globals.json",
    "udoc": "./node_modules/.bin/jsdoc server/routes/user.js -d server/jsdoc"
  },
  "dependencies": {
    "aws-sdk": "^2.1022.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "newman": "^5.3.0"
  }
}
